{
  "name": "create-wordpress-block",
  "version": "0.4.0-2",
  "description": "Generates PHP, JS and CSS code for registering a block for a WordPress plugin or theme.",
  "author": "Grzegorz <grzegorz@gziolo.pl> (Greg)",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "block"
  ],
  "homepage": "https://github.com/gziolo/create-wordpress-block#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gziolo/create-wordpress-block.git"
  },
  "bugs": {
    "url": "https://github.com/gziolo/create-wordpress-block/issues"
  },
  "main": "index.js",
  "files": [
    "templates",
    "*.js"
  ],
  "bin": {
    "create-wordpress-block": "index.js"
  },
  "scripts": {
    "build": "./index.js esnext-example && ./index.js -t es5 es5-example",
    "lint-css": "wp-scripts lint-style",
    "lint-js": "wp-scripts lint-js",
    "test-unit": "wp-scripts test-unit-js",
    "test": "npm run lint-css && npm run lint-js && npm run test-unit"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.0.1",
    "execa": "^3.4.0",
    "inquirer": "^7.0.0",
    "make-dir": "^3.0.0",
    "mustache": "^3.1.0",
    "pkg-install": "^1.0.0",
    "write-pkg": "^4.0.0"
  },
  "devDependencies": {
    "@wordpress/eslint-plugin": "^3.2.0",
    "@wordpress/scripts": "^6.0.0"
  },
  "directories": {
    "test": "test"
  }
}
